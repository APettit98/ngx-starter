{
  "name": "ngx-starter",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "start:prod": "ng serve --proxy-config proxy.conf.json --prod",
    "build": "npm run lint && ng build",
    "build:prod": "npm run lint && ng build --prod",
    "build:bundle-report": "ng build --stats-json && webpack-bundle-analyzer dist/ngx-starter/stats.json",
    "test": "ng test",
    "test:ci": "ng test --watch=false --code-coverage",
    "lint": "ng lint && stylelint './src/**/*.scss'",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.2",
    "@angular/common": "7.2",
    "@angular/compiler": "7.2",
    "@angular/core": "7.2",
    "@angular/forms": "7.2",
    "@angular/http": "7.2",
    "@angular/platform-browser": "7.2",
    "@angular/platform-browser-dynamic": "7.2",
    "@angular/router": "7.2",
    "@ng-select/ng-select": "^2.13.3",
    "bootstrap": "4.3.1",
    "core-js": "2.5",
    "font-awesome": "4.7",
    "moment": "2.22",
    "ngx-bootstrap": "4.2.0",
    "roboto-fontface": "0.10",
    "rxjs": "6.3",
    "zone.js": "0.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.13",
    "@angular/cli": "7.2",
    "@angular/compiler-cli": "7.2",
    "@angular/language-service": "7.2",
    "@types/jasmine": "2.8",
    "@types/jasminewd2": "2.0",
    "@types/node": "8.9",
    "codelyzer": "4.5",
    "jasmine-core": "2.99",
    "jasmine-spec-reporter": "4.2",
    "karma": "4.0",
    "karma-chrome-launcher": "2.2",
    "karma-coverage-istanbul-reporter": "2.0.5",
    "karma-jasmine": "1.1",
    "karma-jasmine-html-reporter": "0.2",
    "karma-junit-reporter": "1.2",
    "nodemon": "1.18.6",
    "protractor": "5.4",
    "stylelint": "9",
    "stylelint-config-recommended-scss": "3",
    "stylelint-scss": "3",
    "ts-node": "7.0",
    "tslint": "5.11",
    "typescript": "3.1",
    "webpack-bundle-analyzer": "3.0"
  }
}
